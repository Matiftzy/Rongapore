{
  "manifest_version": 3,
  "name": "Roblox SG Server Finder",
  "version": "1.0",
  "description": "Otomatis mencari dan join server Roblox di Singapura.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "https://www.roblox.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
